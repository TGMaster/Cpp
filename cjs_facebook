$(document).ready(function()
{
    if(window.location.href.indexOf("m.facebook.com/messages/read/?tid=mid.") != -1)
    {
       if($('#see_older a.touchable')[0])
       {
         setInterval(function()
         {
              $('#see_older .mfsm').click();
         }, 500);
     }
    }
});

//Inspect emote in facebook
var inputs = document.getElementsByClassName('_39n');
for(var i=0; i<inputs.length;i++) { inputs[i].click(); }
